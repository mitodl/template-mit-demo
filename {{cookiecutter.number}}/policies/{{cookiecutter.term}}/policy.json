{
    "course/{{ cookiecutter.term }}": {
        "course_image": "course_image.png",
        "diplay_name": "{{ cookiecutter.name|title }}",
        {% if cookiecutter.number[-1] == 'r' %}
        "display_coursenumber": "{{ cookiecutter.number[:-1] }}",
        "display_organization": "MIT",
        {% elif 'r_' in cookiecutter.number %}
        "display_coursenumber": "{{ cookiecutter.number[:-3] }}",
        "display_organization": "MIT",
        {% else %}
        "display_coursenumber": "{{ cookiecutter.number }}",
        "display_organization": "MITx",
        {% endif %}
        "end": "2130-01-30T00:00",
        "enrollment_end": "2130-01-30T00:00",
        "graceperiod": "0 day 0 hours 15 minutes 0 seconds",
        "rerandomize": "once",
        "start": "2030-01-30T00:00",
        "tabs": [
            {
                "course_staff_only": false, 
                "name": "Home", 
                "type": "course_info"
            }, 
            {
                "course_staff_only": false, 
                "name": "Course", 
                "type": "courseware"
            }, 
            {
                "course_staff_only": false, 
                "name": "Textbooks", 
                "type": "textbooks"
            }, 
            {
                "course_staff_only": false, 
                "name": "Discussion", 
                "type": "discussion"
            }, 
            {
                "course_staff_only": false, 
                "name": "Wiki", 
                "type": "wiki"
            }, 
            {
                "course_staff_only": false, 
                "name": "Progress", 
                "type": "progress"
            }
        ]
    }
}
